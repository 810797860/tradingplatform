<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.secondhand.tradingplatformadminmapper.mapper.admin.business.LoginLogMapper">

		<!-- 通用查询映射结果 -->
			<resultMap id="loginLogMap" type="com.secondhand.tradingplatformadminentity.entity.admin.business.LoginLog" extends="com.secondhand.tradingplatformadminmapper.mapper.BaseEntityMapper.baseEntityMapper">
									
				<result column="logname" property="logname" jdbcType="VARCHAR" />
				<result column="userid" property="userid" jdbcType="INTEGER" />
				<result column="createtime" property="createtime" jdbcType="DATE" />
				<result column="succeed" property="succeed" jdbcType="VARCHAR" />
				<result column="message" property="message" jdbcType="VARCHAR" />
				<result column="ip" property="ip" jdbcType="VARCHAR" />

		</resultMap>

        <!-- 通用查询映射结果 -->
		<resultMap id="loginLogMapSimple" type="com.secondhand.tradingplatformadminentity.entity.admin.business.LoginLog">
																			
                        <result column="logname" property="logname" jdbcType="VARCHAR" />
                        <result column="userid" property="userid" jdbcType="INTEGER" />
                        <result column="createtime" property="createtime" jdbcType="DATE" />
                        <result column="succeed" property="succeed" jdbcType="VARCHAR" />
                        <result column="message" property="message" jdbcType="VARCHAR" />
                        <result column="ip" property="ip" jdbcType="VARCHAR" />

        </resultMap>

    <update id="fakeDeleteById">
        update c_business_login_log set deleted = true
        where id = #{loginLogId}
    </update>

    <update id="fakeBatchDelete" parameterType="java.util.List">
        update c_business_login_log set deleted = true
        where id IN
        <foreach collection="loginLogIds" index="index" item="item" open="(" separator="," close=")">
            #{item}
        </foreach>
    </update>

</mapper>